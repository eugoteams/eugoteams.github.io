        {
            "name": "Navigating in cells",
            "shortcuts": [
                {
                    "description": "Move to the previous cell in a worksheet or the previous option in a dialog",
                    "keys": ["Shift", "Tab"]
                },
                {
                    "description": "Move one cell up in a worksheet",
                    "keys": ["Up"]
                },
                {
                    "description": "Move one cell down in a worksheet",
                    "keys": ["Down"]
                },
                {
                    "description": "Move one cell left in a worksheet",
                    "keys": ["Left"]
                },
                {
                    "description": "Move one cell right in a worksheet",
                    "keys": ["Right"]
                },
                {
                    "description": "Move to the edge of the current data region in a worksheet",
                    "keys": ["Ctrl", "Arrow"]
                },
                {
                    "description": "Move to the last cell in the row or column",
                    "keys": ["End"]
                },
                {
                    "description": "Move to the last cell on a worksheet, to the lowest used row of the rightmost used column",
                    "keys": ["Ctrl", "End"]
                },
                {
