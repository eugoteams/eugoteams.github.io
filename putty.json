        {
          "description": "Delete from under the cursor",
          "keys": ["Ctrl", "D"]
        },
        {
          "description": "Jump to the end of the line",
          "keys": ["Ctrl", "E"]
        },
        {
          "description": "Move forward a character",
          "keys": ["Ctrl", "F"]
        },
        {
          "description": "Delete to end-of-line",
          "keys": ["Ctrl", "K"]
